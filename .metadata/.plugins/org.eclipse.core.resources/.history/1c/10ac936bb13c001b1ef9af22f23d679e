	private static int numberOfTrees(String[] arr) {
		int position= 0;
		int numOfTrees=0;
		int row=0;
		System.out.println(arr[0].length());
		
		while (row < arr.length) {
			System.out.println("position= "+position);
			System.out.println("numOfTrees= "+numOfTrees);
			System.out.println("row= "+row);
			System.out.println(arr[row].charAt(position));
			if (arr[row].charAt(position)=='#')
				numOfTrees++;
			row++;
			position= position+3;
			if (position >= arr[0].length()) {
				position= position - arr[0].length() ;
			}
		}
		return numOfTrees;
	}
